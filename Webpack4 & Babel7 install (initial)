1. npm init -y
2. npm i -D webpack webpack-cli webpack-dev-server
3. npm i -D @babel/core @babel/cli @babel/node @babel/polyfill @babel/preset-env @babel/register babel-loader babel-register html-webpack-plugin
4. npm install --save-dev @babel/plugin-transform-strict-mode
5. npm install --save-dev @babel/plugin-proposal-class-properties
